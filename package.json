  "bugs": {
    "url": "https://github.com/nolanlawson/japanese-name-converter-ui/issues"
  },
  "homepage": "https://github.com/nolanlawson/japanese-name-converter-ui#readme",
  "volta": {
    "node": "16.13.0",
    "npm": "7.24.2"
  },
  "dependencies": {
    "parcel": "^2.0.1",
    "replace": "^1.2.1",
    "rimraf": "^3.0.2"
  }
}
